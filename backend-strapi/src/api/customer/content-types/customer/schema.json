{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "phone": {
      "type": "string",
      "unique": true
    },
    "email": {
      "type": "email",
      "unique": true
    },
    "address": {
      "type": "text",
      "required": true
    },
    "default_shipping_address": {
      "type": "text",
      "required": true
    },
    "billing_address": {
      "type": "text"
    },
    "oders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::oder.oder",
      "mappedBy": "customer"
    }
  }
}
