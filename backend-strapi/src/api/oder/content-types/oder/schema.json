{
  "kind": "collectionType",
  "collectionName": "oders",
  "info": {
    "singularName": "oder",
    "pluralName": "oders",
    "displayName": "Oder"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "amount": {
      "type": "integer",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Requesting",
        "On-track",
        "Packaged",
        "Shipping",
        "Delivered",
        "Cancelled"
      ],
      "default": "Requesting",
      "required": true
    },
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::customer.customer",
      "inversedBy": "oders"
    },
    "oder_details": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::oder-detail.oder-detail",
      "mappedBy": "oder"
    }
  }
}
